{
  "name": "vuejs-masterclass-2024",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "sp:init": "supabase init",
    "sp:login": "supabase login",
    "sp:link": "dotenv -p -- supabase link --project-ref $SUPABASE_PROJECT_ID",
    "debug": "dotenv -- node -e \"console.log(true)\""
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.46.1",
    "pinia": "^2.2.6",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.9.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/tsconfig": "^0.5.1",
    "dotenv-cli": "^7.4.2",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.30.0",
    "npm-run-all2": "^7.0.1",
    "prettier": "^3.3.3",
    "supabase": "^1.219.2",
    "typescript": "~5.6.3",
    "unplugin-vue-router": "^0.10.8",
    "vite": "^5.4.10",
    "vite-plugin-vue-devtools": "^7.5.4",
    "vue-tsc": "^2.1.10"
  }
}
